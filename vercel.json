{
    "version": 2,
    "builds": [
      {
        "src": "Src/bootstrap.js",  
        "use": "@vercel/node",  
        "config": {
          "includeFiles": ["**/*.js", "**/*.json"]  
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",  
        "dest": "Src/bootstrap.js"  
      }
    ]
  }
  